        },
        "description": {
            "@id": "rdfs:comment",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "label": {
            "@id": "rdfs:label",
            "@container": [
                "@language",
                "@set"
            ]
        },
        "comment": {
            "@id": "rdfs:comment",
